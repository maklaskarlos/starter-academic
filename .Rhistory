mutate(L = rowSums(.[2:1070])) %>%
select(week,L) %>%
mutate(weeknumber = substr(week,6,7),
year = substr(week,1,4)) %>%
mutate(weekyear = as.Date(paste( 1, weeknumber, year, sep = "-" ), format = "%u-%U-%Y" ))  %>%
select(weekyear, L)
# F Portal
